<table>

  <tr>
    <th>Date</th>
    <th>Paid to</th>
    <th>Amount</th>
  </tr>
  <% @account.checks.each do |check| %>
  <tr>
  <td><%= check.date %></td>
  <td class = "name"><%= check.to %></td>
  <td><%= number_to_currency(check.amount) %></td>
  <td><%= link_to "delete", check, method: :delete, data: {confirm: "Are you sure?"} %></td>
  </tr>
  <% end %>
</table>